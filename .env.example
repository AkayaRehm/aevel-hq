# B.L.A.S.T. Analytics â€” Environment template
# Copy to .env and set values. Do not commit .env.

# Gemini Free (optional; used by navigation layer for routing/formatting)
GEMINI_API_KEY=

# Data source: set one of these for pipeline runs
DATA_SOURCE_PATH=
DATA_SOURCE_URL=

# Optional: format when using file (json or csv)
DATA_SOURCE_FORMAT=json

# Delivery: webhook URL for report payload (optional)
DELIVERY_WEBHOOK_URL=

# Timeout in seconds for webhook POST
DELIVERY_TIMEOUT_SEC=10

# Server (Render sets PORT automatically)
PORT=10000

# Admin: password to access /admin (control which emails are sent and to whom)
ADMIN_PASSWORD=

# Zoho Mail (free zohomail.com): emails sent from this address
ZOHO_EMAIL=hello.aevel@zohomail.com
ZOHO_PASSWORD=
